<svg width="600" height="400" viewBox="0 0 600 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="600" height="400" fill="url(#heroGradient)"/>
  
  <!-- Road -->
  <path d="M0 350 Q150 320 300 340 Q450 360 600 330" stroke="#F24236" stroke-width="8" fill="none"/>
  <path d="M0 350 Q150 320 300 340 Q450 360 600 330" stroke="white" stroke-width="2" fill="none" stroke-dasharray="20,10"/>
  
  <!-- Car silhouette -->
  <g transform="translate(250, 300)">
    <rect x="0" y="20" width="100" height="30" rx="15" fill="#2E86AB"/>
    <rect x="10" y="10" width="80" height="20" rx="10" fill="#A23B72"/>
    <circle cx="20" cy="50" r="8" fill="#333"/>
    <circle cx="80" cy="50" r="8" fill="#333"/>
  </g>
  
  <!-- License plates floating around -->
  <g transform="translate(100, 100)">
    <rect width="60" height="30" rx="4" fill="white" stroke="#2E86AB" stroke-width="1"/>
    <text x="30" y="20" text-anchor="middle" fill="#2E86AB" font-family="Arial" font-size="8" font-weight="bold">CA</text>
  </g>
  
  <g transform="translate(450, 80)">
    <rect width="60" height="30" rx="4" fill="white" stroke="#2E86AB" stroke-width="1"/>
    <text x="30" y="20" text-anchor="middle" fill="#2E86AB" font-family="Arial" font-size="8" font-weight="bold">TX</text>
  </g>
  
  <g transform="translate(350, 150)">
    <rect width="60" height="30" rx="4" fill="white" stroke="#2E86AB" stroke-width="1"/>
    <text x="30" y="20" text-anchor="middle" fill="#2E86AB" font-family="Arial" font-size="8" font-weight="bold">NY</text>
  </g>
  
  <g transform="translate(150, 200)">
    <rect width="60" height="30" rx="4" fill="white" stroke="#2E86AB" stroke-width="1"/>
    <text x="30" y="20" text-anchor="middle" fill="#2E86AB" font-family="Arial" font-size="8" font-weight="bold">FL</text>
  </g>
  
  <!-- Mountains in background -->
  <polygon points="0,250 100,150 200,250" fill="#A23B72" opacity="0.3"/>
  <polygon points="150,250 250,120 350,250" fill="#2E86AB" opacity="0.3"/>
  <polygon points="300,250 400,140 500,250" fill="#A23B72" opacity="0.3"/>
  <polygon points="450,250 550,100 600,250" fill="#2E86AB" opacity="0.3"/>
  
  <!-- Sun -->
  <circle cx="500" cy="80" r="30" fill="#F24236" opacity="0.8"/>
  <circle cx="500" cy="80" r="20" fill="#F24236"/>
  
  <!-- Clouds -->
  <ellipse cx="150" cy="60" rx="25" ry="15" fill="white" opacity="0.7"/>
  <ellipse cx="170" cy="60" rx="20" ry="12" fill="white" opacity="0.7"/>
  <ellipse cx="350" cy="50" rx="30" ry="18" fill="white" opacity="0.7"/>
  <ellipse cx="375" cy="50" rx="25" ry="15" fill="white" opacity="0.7"/>
  
  <!-- State outline shapes (simplified) -->
  <g transform="translate(50, 250)" opacity="0.2">
    <path d="M0 0 L40 0 L40 30 L0 30 Z" fill="#2E86AB"/>
    <text x="20" y="20" text-anchor="middle" fill="white" font-size="10">CA</text>
  </g>
  
  <g transform="translate(500, 280)" opacity="0.2">
    <path d="M0 0 L35 0 L35 25 L0 25 Z" fill="#A23B72"/>
    <text x="17" y="17" text-anchor="middle" fill="white" font-size="10">TX</text>
  </g>
  
  <!-- Collection progress indicator -->
  <g transform="translate(20, 20)">
    <rect width="200" height="40" rx="20" fill="white" opacity="0.9"/>
    <text x="100" y="18" text-anchor="middle" fill="#2E86AB" font-size="12" font-weight="bold">States Collected</text>
    <text x="100" y="32" text-anchor="middle" fill="#A23B72" font-size="14" font-weight="bold">23 / 50</text>
  </g>
  
  <!-- Achievement badge -->
  <g transform="translate(520, 20)">
    <circle cx="30" cy="30" r="25" fill="#F24236"/>
    <text x="30" y="35" text-anchor="middle" fill="white" font-size="16">üèÜ</text>
  </g>
  
  <defs>
    <linearGradient id="heroGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#F8F9FA;stop-opacity:1" />
    </linearGradient>
  </defs>
</svg> 